compilation_database_dir: _build
output_directory: ../puml
generate_links:
  link: https://github.com/FreeCAD/FreeCAD/blob/{{ git.commit }}/{{ element.source.path }}#L{{ element.source.line }}
  tooltip: "{% if existsIn(element, \"comment\") and existsIn(element.comment, \"brief\") %}{{ abbrv(trim(replace(element.comment.brief.0, \"\\n+\", \" \")), 256) }}{% else %}{{ element.name }}{% endif %}"
diagram_templates:
  app_base_class_context_tmpl:
    description: Generate class context diagrams
    type: class
    template: "app_{{ class_name }}:\n  type: class\n  title: App::{{ class_name }} context\n  generate_packages: true\n  relative_to: src\n  glob:\n    - Base/*.cpp\n    - App/*.cpp\n  include:\n    context:\n      - App::{{ class_name }}\n    paths:\n      - .\n  exclude:\n    relationships:\n      - dependency\n"
diagrams:
  package_namespace_diagram:
    type: package
    title: Main namespaces package diagram
    glob:
      - src/App/*.cpp
      - src/Base/*.cpp
      - src/Gui/*.cpp
      - src/Gui/**/*.cpp
      - src/Main/*.cpp
    include:
      paths:
        - src
    exclude:
      namespaces:
        - std
  package_directory_diagram:
    type: package
    title: Directory structure package diagram
    package_type: directory
    relative_to: src
    glob:
      - App/*.cpp
      - Base/*.cpp
      - Gui/*.cpp
      - Gui/**/*.cpp
      - Main/*.cpp
    include:
      paths:
        - .
  base_class_diagram:
    type: class
    title: Base classes
    generate_packages: true
    relative_to: src
    glob:
      - Base/*.cpp
    include:
      paths:
        - Base
    exclude:
      access: [public, protected, private]
      relationships:
        - dependency
      elements:
        - Base::Quantity
    plantuml:
      before:
        - left to right direction
  nodeitem_hierarchy_diagram:
    type: class
    title: NodeItem class hierarchy
    generate_packages: true
    relative_to: src
    glob:
      - Base/*.cpp
    include:
      paths:
        - Base
        - App
      subclasses:
        - Base::NodeItem
      relationships:
        - inheritance
    exclude:
      access: [public, protected, private]
    plantuml:
      before:
        - left to right direction
  baseclass_hierarchy_diagram:
    type: class
    title: BaseClass class hierarchy
    generate_packages: true
    relative_to: src
    glob:
      - Base/*.cpp
      - App/*.cpp
    include:
      paths:
        - Base
        - App
      subclasses:
        - Base::BaseClass
      relationships:
        - inheritance
    exclude:
      access: [public, protected, private]
    plantuml:
      before:
        - left to right direction
  app_Annotation:
    type: class
    title: App::Annotation context
    generate_packages: true
    relative_to: src
    glob:
      - Base/*.cpp
      - App/*.cpp
    include:
      context:
